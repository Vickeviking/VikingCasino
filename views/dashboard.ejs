          <button type='button' onclick="fn1()"  id="send-button"> send </button>
        </form>


      </div>
      <div class="grid-itemBildspel">
        <i class="fas fa-arrow-right" id="nextBtn"></i>

        <i class="fas fa-arrow-left" id="prevBtn"></i>

        <div class="carousel-slide">
          <img src="../img/getRitch.jpg" id="lastClone" alt="get ritch" />

          <img src="../img/poker.jpg" alt="poker" />

          <img src="../img/roulett.jpg" alt="roulett" />

