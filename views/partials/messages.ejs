<% if(typeof errors != 'undefined') { %>
    <% errors.forEach(function(error){ %>
        <div class="errormsg">
            <%= error.msg %>  
          </div>
 

    <% }); %>
<% } %>

<% if(success_msg != '') {%>
    <div class="successmsg">
        <%= success_msg %>  
      </div>
<%} %>


<% if(error_msg != '') {%>
    <div class="successmsg">
        <%= error_msg %>  
      </div>
<%} %>